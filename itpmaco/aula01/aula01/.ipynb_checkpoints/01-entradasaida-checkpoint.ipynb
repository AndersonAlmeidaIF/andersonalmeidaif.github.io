   "source": [
    "nome = input(\"Informe seu nome: \")\n",
    "idade = int(input(\"Infome a idade: \"))\n",
    "peso = float(input(\"Informe o peso: \"))\n",
    "altura = float(input(\"Informe a altura: \"))\n",
    "\n",
    "imc = peso / pow(altura, 2)\n",
    "\n",
    "print(\"Nome:\", nome)\n",
    "print(\"Idade:\", idade)\n",
    "print(\"IMC:\", imc)"
   ]
